{
  "name": "stealth-edtech",
  "version": "1.0.0",
  "description": "A tree traversal solution using Node typescript, mongo db and aggregation",
  "main": "index.js",
  "author": "Andy Ogaga",
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc -p .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:prod": "node dist/src/index.j",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --runInBand --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@sentry/node": "^5.27.6",
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/mongoose": "5.7.32",
    "@types/node": "^14.11.5",
    "body-parser": "^1.19.0",
    "convert-excel-to-json": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "mongoose": "5.9.25",
    "morgan": "^1.10.0",
    "ts-loader": "^8.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@types/convert-excel-to-json": "^1.7.0",
    "@types/express-fileupload": "^1.1.5",
    "@types/jest": "^26.0.16",
    "@types/morgan": "^1.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "babel-jest": "^26.6.3",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  }
